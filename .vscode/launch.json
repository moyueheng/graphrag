{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debugger: Current File",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"name": "Attach to Node Functions",
			"type": "node",
			"request": "attach",
			"port": 9229,
			"preLaunchTask": "func: host start"
		},
		{
			"name": "Python: GraphRAG Index",
			"type": "debugpy",
			"request": "launch",
			"module": "graphrag.index",
			"args": [
				"--root",
				"./ragtest",
				// "--nocache"
			],
			"cwd": "${workspaceFolder}",
			"env": {
				"PYTHONPATH": "${workspaceFolder}"
			},
			"console": "integratedTerminal"
		},
	]
}